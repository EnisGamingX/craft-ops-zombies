{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2022",
    "rootDir": "./Craft Ops I/source",
    "outDir": "./.regolith/tmp/BP/scripts/",
    "moduleResolution": "Node",
    "paths": {
      "@minecraft/server": [
        "./node_modules/@minecraft/server"
      ],
      "@minecraft/server-ui": [
        "./node_modules/@minecraft/server-ui"
      ],
      "@minecraft/server-gametest": [
        "./node_modules/@minecraft/server-gametest"
      ]
    },
    "lib": [
      "DOM",
      "ES2020",
    ],
    "allowJs": false,
    "declaration": false,
    "sourceMap": false,
    "removeComments": false,
    "forceConsistentCasingInFileNames": false,
    "noImplicitAny": false,
    "skipLibCheck": false,
    "strict": false
  },
}